{
  "name": "hokusai-get-started",
  "version": "1.0.0",
  "description": "a project that get started with Hokusai API",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "store-metadata": "run-s tsc store-metadata:run",
    "store-metadata:run": "node ./build/storeMetadata.js",
    "mint-nft": "yarn tsc && yarn mint-nft:run",
    "mint-nft:run": "node ./build/mintNft.js",
    "get-nft": "yarn tsc && yarn get-nft:run",
    "get-nft:run": "node ./build/getNft.js",
    "transfer-nft": "yarn tsc && yarn transfer-nft:run",
    "transfer-nft:run": "node ./build/transferNft.js",
    "set-royalty": "yarn tsc && yarn set-royalty:run",
    "set-royalty:run": "node ./build/setRoyalty.js",
    "get-royalty": "yarn tsc && yarn get-royalty:run",
    "get-royalty:run": "node ./build/getRoyalty.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "ethers": "^5.4.6",
    "nft.storage": "^3.1.3",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.12",
    "typescript": "^4.3.5",
    "yarn-run-all": "^3.1.1"
  }
}
