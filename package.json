{
  "name": "hokusai-get-started",
  "version": "2.0.0",
  "description": "a project that get started with Hokusai",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",

    "store-metadata": "yarn store-metadata:v2",
    "store-metadata:v1": "yarn tsc && yarn store-metadata:v1:run",
    "store-metadata:v2": "yarn tsc && yarn store-metadata:v2:run",
    "store-metadata:v1:run": "node ./build/v1/storeMetadata.js",
    "store-metadata:v2:run": "node ./build/v2/storeMetadata.js",

    "mint-nft": "yarn mint-nft:v2",
    "mint-nft:v1": "yarn tsc && yarn mint-nft:v1:run",
    "mint-nft:v2": "yarn tsc && yarn mint-nft:v2:run",
    "mint-nft:v1:run": "node ./build/v1/mintNft.js",
    "mint-nft:v2:run": "node ./build/v2/mintNft.js",

    "get-nft": "yarn get-nft:v2",
    "get-nft:v1": "yarn tsc && yarn get-nft:v1:run",
    "get-nft:v2": "yarn tsc && yarn get-nft:v2:run",
    "get-nft:v1:run": "node ./build/v1/getNft.js",
    "get-nft:v2:run": "node ./build/v2/getNft.js",

    "transfer-nft": "yarn transfer-nft:v2",
    "transfer-nft:v1": "yarn tsc && yarn transfer-nft:v1:run",
    "transfer-nft:v2": "yarn tsc && transfer-nft:v2:run",
    "transfer-nft:v1:run": "node ./build/v1/transferNft.js",
    "transfer-nft:v2:run": "node ./build/v2/transferNft.js",

    "burn-nft": "yarn burn-nft:v2",
    "burn-nft:v1": "yarn tsc && yarn burn-nft:v1:run",
    "burn-nft:v2": "yarn tsc && yarn burn-nft:v2:run",
    "burn-nft:v1:run": "node ./build/v1/burnNft.js",
    "burn-nft:v2:run": "node ./build/v2/burnNft.js",

    "set-royalty": "yarn set-royalty:v2",
    "set-royalty:v1": "yarn tsc && yarn set-royalty:v1:run",
    "set-royalty:v2": "yarn tsc && yarn set-royalty:v2:run",
    "set-royalty:v1:run": "node ./build/v1/setRoyalty.js",
    "set-royalty:v2:run": "node ./build/v2/setRoyalty.js",

    "get-royalty": "yarn get-royalty:v2",
    "get-royalty:v1": "yarn tsc && yarn get-royalty:v1:run",
    "get-royalty:v2": "yarn tsc && yarn get-royalty:v2:run",
    "get-royalty:v1:run": "node ./build/v1/getRoyalty.js",
    "get-royalty:v2:run": "node ./build/v2/getRoyalty.js",

    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "ethers": "^5.4.6",
    "nft.storage": "^3.1.3",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.12",
    "typescript": "^4.3.5",
    "yarn-run-all": "^3.1.1"
  }
}
